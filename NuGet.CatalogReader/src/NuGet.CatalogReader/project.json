{
  "version": "1.0.0",

  "description": "A NuGet catalog reader for discovering all packages on a v3 feed.",

  "packOptions": {
    "tags": [
      "nuget",
      "catalog",
      "v3"
    ],
    "authors": [
      "juste"
    ]
  },

  "buildOptions": {
    "warningsAsErrors": true,
    "xmlDoc": true
  },

  "dependencies": {
    "Newtonsoft.Json": "9.0.1",
    "NuGet.Versioning": "3.5.0-beta2-1484"
  },

  "frameworks": {
    "netstandard1.3": {
      "dependencies": {
        "NETStandard.Library": "1.6.0"
      }
    },
    "net45": {
      "frameworkAssemblies": {
        "System.Net.Http": "4.0.0.0"
      }
    }
  }
}
